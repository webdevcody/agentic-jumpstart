{
  "name": "tanstack-start-example-basic-react-query",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "dev": "npm run db:up && vite dev",
    "build": "vite build",
    "start": "npm run db:migrate && node .output/server/index.mjs",
    "start:test": "npm run db:migrate:test && cross-env PORT=3001 HOST_NAME=http://localhost:3001 IS_TEST=true DATABASE_URL_TEST=postgresql://postgres:example@localhost:5433/postgres node .output/server/index.mjs",
    "db:push": "drizzle-kit push --config=drizzle.config.ts",
    "db:migrate": "tsx ./src/db/migrate.ts",
    "db:migrate:test": "cross-env IS_TEST=true DATABASE_URL=postgresql://postgres:example@localhost:5433/postgres tsx ./src/db/migrate.ts",
    "db:generate": "drizzle-kit generate --config=drizzle.config.ts",
    "db:studio": "drizzle-kit studio",
    "db:clear": "tsx ./src/db/clear.ts",
    "db:seed": "tsx ./src/db/seed.ts",
    "db:reset": "npm run db:clear && npm run db:migrate && npm run db:seed",
    "db:up": "docker compose up -d",
    "db:down": "docker compose down",
    "db:logs": "docker compose logs -f",
    "stripe:listen": "stripe listen --forward-to http://localhost:4000/api/stripe/webhook",
    "upload:videos": "tsx ./scripts/upload-videos-to-r2.ts",
    "test": "playwright test",
    "test:ui": "playwright test --ui",
    "test:headed": "playwright test --headed",
    "test:debug": "playwright test --debug",
    "test:report": "playwright show-report",
    "test:install": "playwright install",
    "test:chrome": "playwright test --project=chromium",
    "test:firefox": "playwright test --project=firefox",
    "test:mobile": "playwright test --project='Mobile Chrome'",
    "test:db:setup": "tsx ./tests/setup/database.ts",
    "test:no-webkit": "playwright test --config=playwright.config.no-webkit.ts",
    "test:unit": "vitest run",
    "test:unit:watch": "vitest",
    "test:unit:coverage": "vitest run --coverage"
  },
